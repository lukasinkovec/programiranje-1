    "version": "2.0.0",
    "tasks": [
        {
            "label": "OCaml",
            "type": "shell",
            "command": "C:\\OCaml\\bin\\ocaml -init \\\"${file}\\\"",
            "problemMatcher": [
                "$ocamlc"
            ]
        },
        {
            "label": "Python",
            "type": "shell",
            "command": "python -i \\\"${file}\\\"",
            "problemMatcher": [
                "$ocamlc"
            ]
        }
    ]
}